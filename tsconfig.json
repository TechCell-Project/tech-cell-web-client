{
    "compilerOptions": {
        "target": "es5",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "esModuleInterop": true,
        "module": "esnext",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "incremental": true,
        "plugins": [
            {
                "name": "next"
            }
        ],
        "baseUrl": "./src/",
        "paths": {
            "@public": ["../public"],
            "@public/*": ["../public/*"],
            "@action": ["actions"],
            "@action/*": ["actions/*"],
            "@styles": ["styles"],
            "@styles/*": ["styles/*"],
            "@app": ["app/(authorized)", "app/(unauthorized)", "app"],
            "@app/*": ["app/(authorized)/*", "app/(unauthorized)/*", "app/*"],
            "@components": ["components"],
            "@components/*": ["components/*"],
            "@models": ["models"],
            "@models/*": ["models/*"],
            "@store": ["store"],
            "@store/*": ["store/*"],
            "@services": ["services"],
            "@services/*": ["services/*"],
            "@constants": ["constants"],
            "@constants/*": ["constants/*"],
            "@interfaces": ["interfaces"],
            "@interfaces/*": ["interfaces/*"],
            "@hooks": ["hooks"],
            "@hooks/*": ["hooks/*"],
            "@libs": ["libs"],
            "@libs/*": ["libs/*"],
            "@config": ["config"],
            "@config/*": ["config/*"],
            "@utils": ["utils"],
            "@utils/*": ["utils/*"]
        }
    },
    "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
    "exclude": ["node_modules"]
}
